<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.durgam.guerra.modelo.RequisitoResuelto" table="requisito_resuelto" catalog="requisitoxmp" optimistic-lock="version">
        <id name="id" type="long">
            <column name="id" />
            <generator class="foreign">
                <param name="property">estadoRequisito</param>
            </generator>
        </id>
        <one-to-one name="estadoRequisito" class="com.durgam.guerra.modelo.EstadoRequisito" constrained="true"></one-to-one>
    </class>
</hibernate-mapping>
